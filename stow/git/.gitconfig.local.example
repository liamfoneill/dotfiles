# Machine-specific git identity.
# Copy this file to ~/.gitconfig.local and fill in your details.
#
# Signing is disabled by default so git works immediately on a fresh machine.
# Once 1Password is installed and your SSH key is configured, set gpgsign = true.
#
# Personal:
#   [user]
#       name = Liam O'Neill
#       email = liamfoneill@users.noreply.github.com
#       signingkey = ssh-ed25519 YOUR_PERSONAL_KEY
#   [commit]
#       gpgsign = true
#
# Work:
#   [user]
#       name = Liam O'Neill
#       email = 143192425+liamoneill-stripe@users.noreply.github.com
#       signingkey = ssh-ed25519 YOUR_WORK_KEY
#   [commit]
#       gpgsign = true

[user]
    name = REPLACE_ME
    email = REPLACE_ME
    signingkey = ssh-ed25519 REPLACE_ME

[commit]
    gpgsign = false
